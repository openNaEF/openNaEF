<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <!-- These settings is only for Phalanx Client UI. -->

    <!-- Attribute name to store the value for inventory ID -->
    <id-field-name>PWID</id-field-name>

    <!-- Attribute name for storing the value of the accommodating LSP -->
    <related-lsp-field-name>rsvp-lsps</related-lsp-field-name>

    <!-- Attribute name to prepare choice of operation state -->
    <operation-status-field-names>
        <value>Operational Status</value>
    </operation-status-field-names>

    <!-- List display target item
         value: [OBJECT_TYPE] Distinguished name in RenderingUtil -->
    <fields>
        <value>PWID</value>
        <value>PW Name</value>
        <value>Operational Status</value>
        <value>Facility Status</value>
        <value>rsvp-lsps</value>
        <value>XePhion ID</value>
        <value>End node(A)</value>
        <value>End port(A)</value>
        <value>End node(B)</value>
        <value>End port(B)</value>
    </fields>

    <!-- Property display target item
         value: [OBJECT_TYPE] Distinguished name in RenderingUtil -->
    <property-fields>
        <value>PWID</value>
        <value>PW Name</value>
        <value>Operational Status</value>
        <value>Facility Status</value>
        <value>rsvp-lsps</value>
        <value>ServiceID</value>
        <value>End node(A)</value>
        <value>End port(A)</value>
        <value>End node(B)</value>
        <value>End port(B)</value>
        <value>Note</value>
    </property-fields>

    <!-- Filter view search item
         value: [OBJECT_TYPE] Distinguished name in RenderingUtil -->
    <filtering-fields>
        <value>PWID</value>
        <value>PW Name</value>
        <value>Operational Status</value>
        <value>Facility Status</value>
        <value>rsvp-lsps</value>
        <value>ServiceID</value>
        <value>End node(A)</value>
        <value>End port(A)</value>
        <value>End node(B)</value>
        <value>End port(B)</value>
        <value>Note</value>
    </filtering-fields>

    <!-- Filter view Search type of search item
         key: [OBJECT_TYPE] Distinguished name in RenderingUtil
         value: Search type [contain/select/equal/numeric] -->
    <condition-of-filtering>
        <prop key="PWID">contain</prop>
        <prop key="PW Name">contain</prop>
        <prop key="Operational Status">select</prop>
        <prop key="Facility Status">select</prop>
        <prop key="rsvp-lsps">contain</prop>
        <prop key="ServiceID">contain</prop>
        <prop key="End node(A)">contain</prop>
        <prop key="End port(A)">contain</prop>
        <prop key="End node(B)">contain</prop>
        <prop key="End port(B)">contain</prop>
        <prop key="Note">contain</prop>
    </condition-of-filtering>

    <!-- Display string on UI
         key: [OBJECT_TYPE] Distinguished name in RenderingUtil
         value: Display Name -->
    <display-name>
        <prop key="PWID">PWID</prop>
        <prop key="PW Name">PW Name</prop>
        <prop key="Operational Status">Operational Status</prop>
        <prop key="Facility Status">Facility Status</prop>
        <prop key="rsvp-lsps">Accommodation LSP</prop>
        <prop key="ServiceID">ServiceID</prop>
        <prop key="End node(A)">End node(A)</prop>
        <prop key="End port(A)">End port(A)</prop>
        <prop key="End node(B)">End node(B)</prop>
        <prop key="End port(B)">End port(B)</prop>
        <prop key="Note">Note</prop>
    </display-name>
</configuration>
